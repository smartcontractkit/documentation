---
const ImportPackage021 = await Astro.glob("./api-reference-callout/ImportPackage021.mdx")
const ImportPackageComponent021 = ImportPackage021[0].Content
const ImportPackage022 = await Astro.glob("./api-reference-callout/ImportPackage022.mdx")
const ImportPackageComponent022 = ImportPackage022[0].Content
const ImportPackage023 = await Astro.glob("./api-reference-callout/ImportPackage023.mdx")
const ImportPackageComponent023 = ImportPackage023[0].Content

export type Props = {
  callout?: "importPackage" | "importPackage021" | "importPackage022" | "importPackage023"
}
const { callout } = Astro.props as Props
---

{callout === "importPackage" && <ImportPackageComponent023 />}
{callout === "importPackage021" && <ImportPackageComponent021 />}
{callout === "importPackage022" && <ImportPackageComponent022 />}
{callout === "importPackage023" && <ImportPackageComponent023 />}
