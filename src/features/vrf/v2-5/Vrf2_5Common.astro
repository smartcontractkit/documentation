---
import UICalloutComponent from "./ui-callout.mdx"
import SecurityComponent from "./security.mdx"
import SupportedNetworksComponent from "./supported-networks.mdx"

export type Props = {
  callout?: "uicallout" | "security" | "supportednetworks"
}
const { callout } = Astro.props as Props
---

{callout === "uicallout" && <UICalloutComponent />}
{callout === "security" && <SecurityComponent />}
{callout === "supportednetworks" && <SupportedNetworksComponent />}
