---
import SenderContractCalloutComponent from "./SenderContractCallout.mdx"
import ThirdPartyAppsComponent from "./third-party-apps.mdx"
import SupportedNetworksConceptsComponent from "./Supported-networks-concepts.mdx"
import ImportCCIPPackageComponent150 from "./api-reference-callout/ImportCCIPPackage150.mdx"
import ImportCCIPPackageComponent151 from "./api-reference-callout/ImportCCIPPackage151.mdx"
import TalkToExpertComponent from "./Talk-Expert.mdx"
import UseSimulatorComponent from "./Use-Simulator.mdx"
import RMNCautionComponent from "./RMNCaution.mdx"
import GasSpikeComponent from "./GasSpike.mdx"
import RegisterFromEOAIntroComponent from "./RegisterFromEOAIntro.mdx"
import RegisterLockMintFromEOAIntroComponent from "./RegisterLockMintFromEOAIntro.mdx"
import GasLimitPoolComponent from "./GasLimitPool.mdx"
import TokenPoolOwnershipComponent from "./TokenPoolOwnership.mdx"
import EducationalDisclaimerComponent from "./EducationalDisclaimer.mdx"

export type Props = {
  callout?:
    | "senderContractCallout"
    | "thirdPartyApps"
    | "supportedNetworksConcepts"
    | "importCCIPPackage"
    | "importCCIPPackage150"
    | "importCCIPPackage151"
    | "talkToExpert"
    | "useSimulator"
    | "rmnCaution"
    | "gasSpike"
    | "registerFromEOAIntro"
    | "registerLockMintFromEOAIntro"
    | "gasLimitPool"
    | "tokenPoolOwnership"
    | "educationalDisclaimer"
}
const { callout } = Astro.props as Props
---

{callout === "senderContractCallout" && <SenderContractCalloutComponent />}
{callout === "thirdPartyApps" && <ThirdPartyAppsComponent />}
{callout === "supportedNetworksConcepts" && <SupportedNetworksConceptsComponent />}
{callout === "importCCIPPackage" && <ImportCCIPPackageComponent150 />}
{callout === "importCCIPPackage150" && <ImportCCIPPackageComponent150 />}
{callout === "importCCIPPackage151" && <ImportCCIPPackageComponent151 />}
{callout === "talkToExpert" && <TalkToExpertComponent />}
{callout === "useSimulator" && <UseSimulatorComponent />}
{callout === "rmnCaution" && <RMNCautionComponent />}
{callout === "gasSpike" && <GasSpikeComponent />}
{callout === "registerFromEOAIntro" && <RegisterFromEOAIntroComponent />}
{callout === "registerLockMintFromEOAIntro" && <RegisterLockMintFromEOAIntroComponent />}
{callout === "gasLimitPool" && <GasLimitPoolComponent />}
{callout === "tokenPoolOwnership" && <TokenPoolOwnershipComponent />}
{callout === "educationalDisclaimer" && <EducationalDisclaimerComponent />}
